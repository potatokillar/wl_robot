<?xml version="1.0" ?>
<robot name="odom_to_base" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="base_link" />

  <joint name="laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="laser_link"/>
    <origin rpy="0 0 0" xyz="0.25 0 0.09"/>
  </joint>

  <link name="laser_link" />

  <joint name="imu_joint" type="fixed">
    <parent link="laser_link"/>
    <child link="imu_link"/>
    <origin rpy="0 0 0" xyz="0.011 0.02329 -0.04412"/>
  </joint>

  <link name="imu_link" />

  <joint name="odom_joint" type="fixed">
    <parent link="laser_link"/>
    <child link="odom"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <link name="odom" />

</robot>